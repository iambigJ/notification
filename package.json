    "debug": "~2.6.9",
    "dompurify": "^2.3.6",
    "dotenv": "^10.0.0",
    "express": "~4.16.1",
    "express-session": "^1.17.3",
    "express-validator": "^6.12.1",
    "get-video-duration": "^4.0.0",
    "http": "^0.0.1-security",
    "ioredis": "^4.28.0",
    "jsdom": "^19.0.0",
    "jsonwebtoken": "^8.5.1",
    "lodash": "^4.17.21",
    "moment": "^2.29.3",
    "mongoose": "^6.5.2",
    "morgan": "~1.9.1",

{
  "name": "akam-node-template",
  "version": "0.0.0",
  "author": "AKAM",
  "private": true,
  "scripts": {
    "start": "nodemon -L bin/www",
    "startServer": "concurrently \"pm2 start bin/www --name akam-node-template\" \"pm2 start microservices/example-microservice/bin/www --name example-microservice\"",
    "development:main": "nodemon -L bin/www",
    "development:all": "concurrently \"nodemon bin/www\" \"nodemon microservices/exaple-microservice/bin/www\"",
    "development:update": "git pull --all; npm i; npx sequelize-cli db:migrate",
    "server:update": "npm i; npx sequelize-cli db:migrate; pm2 restart akam-node-template"
  },
